{
  "name": "Test API",
  "use_keyless": true,
  "version_data": {
    "not_versioned": true,
    "versions": {
      "Default": {
        "name": "Default",
        "use_extended_paths": true,
        "extended_paths": {
          "white_list": [
            {% for endpoint in endpoints %}
              {
                  "path": "{{ endpoint.endpoint_path }}",
                  "ignore_case": false,
                  "method_actions": {
                      "{{ endpoint.method.value }}": {
                          "action": "no_action",
                          "code": 200,
                          "data": "",
                          "headers": {}
                      }
                  }
              }{% if not loop.last %},{% endif %}
            {%- endfor %}
          ]
        }
      }
    }
  },
  "proxy": {
    "listen_path": "{{ listen_path }}",
    "target_url": "{{ host }}{{ listen_path }}",
    "strip_listen_path": true
  },
  "active": true
}
